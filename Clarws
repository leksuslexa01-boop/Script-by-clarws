local NC,AFK,FLC,XP={},{},{},{}
local FBConn,NFConn,XRConn,FNAFESPConn=nil,nil,nil,nil
local FNAFESPModels={}

local function N(t,p)local n=Instance.new(t)for k,v in pairs(p or{})do n[k]=v end return n end
local function Tw(o,p,d)T:Create(o,TweenInfo.new(d or 0.35,Enum.EasingStyle.Quad),p):Play()end

-- GUI
local SG=N("ScreenGui",{Name="UHv8",Parent=C,ResetOnSpawn=false,ZIndexBehavior=Enum.ZIndexBehavior.Sibling})
local MF=N("Frame",{Size=UDim2.new(0,0,0,0),Position=UDim2.new(0.5,0,0.5,0),BackgroundColor3=Color3.fromRGB(10,14,22),ClipsDescendants=true,Visible=false,Parent=SG})
N("UICorner",{CornerRadius=UDim.new(0,16),Parent=MF})

-- Animated holographic background
local HB=N("Frame",{Size=UDim2.new(1,0,1,0),BackgroundTransparency=1,ClipsDescendants=true,Parent=MF})
local Lines={}for i=1,6 do Lines[i]=N("Frame",{Size=UDim2.new(1,0,0,1),Position=UDim2.new(0,0,i/7,0),BackgroundColor3=Color3.fromRGB(0,200,255),BackgroundTransparency=0.88,Parent=HB})end
local Dots={}for i=1,10 do Dots[i]=N("Frame",{Size=UDim2.new(0,3,0,3),Position=UDim2.new(math.random()*0.9,0,math.random()*0.9,0),BackgroundColor3=Color3.fromRGB(0,255,180),BackgroundTransparency=0.6,Parent=HB})N("UICorner",{CornerRadius=UDim.new(1,0),Parent=Dots[i]})end

spawn(function()while true do for i=1,6 do Tw(Lines[i],{Position=UDim2.new(0,0,i/7+math.sin(tick()*0.5+i)*0.015,0)},1.2)end for i=1,10 do Tw(Dots[i],{Position=UDim2.new(math.random()*0.9,0,math.random()*0.9,0)},3)end wait(1)end end)

N("ImageLabel",{Size=UDim2.new(1,40,1,40),Position=UDim2.new(0.5,0,0.5,0),AnchorPoint=Vector2.new(0.5,0.5),BackgroundTransparency=1,Image="rbxassetid://6015897843",ImageColor3=Color3.fromRGB(0,150,255),ImageTransparency=0.8,ScaleType=Enum.ScaleType.Slice,SliceCenter=Rect.new(49,49,450,450),ZIndex=0,Parent=MF})

