N("Frame",{Size=UDim2.new(1,0,0,20),Position=UDim2.new(0,0,1,-10),BackgroundColor3=Color3.fromRGB(18,26,40),BorderSizePixel=0,Parent=TB})
N("TextLabel",{Size=UDim2.new(0.6,0,1,0),Position=UDim2.new(0,12,0,0),BackgroundTransparency=1,Text="рџ”Ґ UNIVERSAL HUB",TextColor3=Color3.fromRGB(0,230,255),TextSize=14,Font=Enum.Font.GothamBlack,Parent=TB})

