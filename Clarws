local CF={}
local TabBtns={}
local function MkTab(n,t,x)
	local b=N("TextButton",{Size=UDim2.new(0.12,0,1,-4),Position=UDim2.new(x,0,0,2),BackgroundTransparency=1,Text=t,TextColor3=Color3.fromRGB(140,150,170),TextSize=9,Font=Enum.Font.GothamBold,Parent=TC})
	TabBtns[n]=b
	CF[n]=N("ScrollingFrame",{Size=UDim2.new(1,-12,1,-74),Position=UDim2.new(0,6,0,72),BackgroundTransparency=1,ScrollBarThickness=3,ScrollBarImageColor3=Color3.fromRGB(0,180,255),CanvasSize=UDim2.new(0,0,0,800),Visible=false,Parent=MF})
	N("UIListLayout",{Padding=UDim.new(0,5),Parent=CF[n]})
	return b
end

MkTab("Server","SERVER",0.015)MkTab("Player","PLAYER",0.135)MkTab("Vision","VISION",0.255)MkTab("Combat","COMBAT",0.375)MkTab("Interface","UI",0.495)MkTab("Extra","MORE",0.615)MkTab("FNAF","FNAF",0.735)MkTab("Smash","SMASH",0.855)MkTab("Knockout","KO",0.975)

local CurTab=nil
local function SwTab(t)
	if CurTab==t then return end
	CurTab=t
	for n,c in pairs(CF)do c.Visible=(n==t)end
	local pos=({Server=0.015,Player=0.185,Vision=0.355,Combat=0.525,Interface=0.695,Extra=0.865,FNAF=0.015,Smash=0.135,Knockout=0.255})[t]
	Tw(TI,{Position=UDim2.new(pos,0,1,-2)},0.25)
	for _,b in pairs(TabBtns)do Tw(b,{TextColor3=Color3.fromRGB(140,150,170)},0.2)end
	Tw(TabBtns[t],{TextColor3=Color3.fromRGB(0,230,255)},0.2)
end

-- Toggle button
local TGB=N("TextButton",{Size=UDim2.new(0,V.BS,0,V.BS),Position=UDim2.new(0,20,0.5,-30),BackgroundColor3=Color3.fromRGB(22,30,45),Text="рџ”Ґ",TextSize=22,Font=Enum.Font.GothamBold,AutoButtonColor=false,Parent=SG})
N("UICorner",{CornerRadius=UDim.new(0,16),Parent=TGB})
N("UIStroke",{Color=Color3.fromRGB(0,200,255),Thickness=2,Parent=TGB})

local function Drg(f)local d,s,p0=false,nil,nil
	f.InputBegan:Connect(function(i)if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then d,s,p0=true,i.Position,f.Position end end)
	f.InputChanged:Connect(function(i)if d and(i.UserInputType==Enum.UserInputType.MouseMovement or i.UserInputType==Enum.UserInputType.Touch)then local o=i.Position-s f.Position=UDim2.new(p0.X.Scale,p0.X.Offset+o.X,p0.Y.Scale,p0.Y.Offset+o.Y)end end)
	f.InputEnded:Connect(function()d=false end)
end
Drg(TGB)

-- GUI toggle
local Op=false
local function TgG()
	Op=not Op
	if Op then MF.Visible=true Tw(MF,{Size=UDim2.new(0,340,0,400),Position=UDim2.new(0.5,-170,0.5,-200)},0.35)if not CurTab then SwTab("Player")end
	else Tw(MF,{Size=UDim2.new(0,0,0,0),Position=UDim2.new(0.5,0,0.5,0)},0.25)task.delay(0.25,function()if not Op then MF.Visible=false end end)end
end

TGB.MouseButton1Click:Connect(TgG)
CB.MouseButton1Click:Connect(function()Op=false TgG()end)
U.InputBegan:Connect(function(i,g)if not g and(i.KeyCode==Enum.KeyCode.RightShift or i.KeyCode==Enum.KeyCode.DPadDown)then TgG()end end)

for n,b in pairs(TabBtns)do b.MouseButton1Click:Connect(function()SwTab(n)end)end

-- UI creators
local function CT(p,n,i,cb)
	local f=N("Frame",{Size=UDim2.new(1,0,0,42),BackgroundColor3=Color3.fromRGB(24,32,48),Parent=p})N("UICorner",{CornerRadius=UDim.new(0,10),Parent=f})
	N("TextLabel",{Size=UDim2.new(0,26,0,26),Position=UDim2.new(0,10,0.5,-13),BackgroundTransparency=1,Text=i,TextSize=15,Parent=f})
	N("TextLabel",{Size=UDim2.new(0.5,0,1,0),Position=UDim2.new(0,40,0,0),BackgroundTransparency=1,Text=n,TextColor3=Color3.new(1,1,1),TextSize=12,Font=Enum.Font.Gotham,TextXAlignment=Enum.TextXAlignment.Left,Parent=f})
	local b=N("TextButton",{Size=UDim2.new(0,44,0,22),Position=UDim2.new(1,-56,0.5,-11),BackgroundColor3=Color3.fromRGB(55,65,85),Text="",AutoButtonColor=false,Parent=f})N("UICorner",{CornerRadius=UDim.new(1,0),Parent=b})
	local k=N("Frame",{Size=UDim2.new(0,16,0,16),Position=UDim2.new(0,3,0.5,-8),BackgroundColor3=Color3.new(1,1,1),Parent=b})N("UICorner",{CornerRadius=UDim.new(1,0),Parent=k})
	local e=false
	local function upd()if e then Tw(b,{BackgroundColor3=Color3.fromRGB(0,210,100)},0.2)Tw(k,{Position=UDim2.new(0,25,0.5,-8)},0.2)else Tw(b,{BackgroundColor3=Color3.fromRGB(55,65,85)},0.2)Tw(k,{Position=UDim2.new(0,3,0.5,-8)},0.2)end cb(e)end
	b.MouseButton1Click:Connect(function()e=not e upd()end)
end

local function CS(p,n,i,mn,mx,d,cb)
	local f=N("Frame",{Size=UDim2.new(1,0,0,56),BackgroundColor3=Color3.fromRGB(24,32,48),Parent=p})N("UICorner",{CornerRadius=UDim.new(0,10),Parent=f})
	local lb=N("TextLabel",{Size=UDim2.new(1,-14,0,18),Position=UDim2.new(0,10,0,6),BackgroundTransparency=1,Text=i.." "..n..": "..d,TextColor3=Color3.new(1,1,1),TextSize=11,Font=Enum.Font.Gotham,TextXAlignment=Enum.TextXAlignment.Left,Parent=f})
	local bg=N("Frame",{Size=UDim2.new(1,-14,0,8),Position=UDim2.new(0,10,0,32),BackgroundColor3=Color3.fromRGB(50,58,75),Parent=f})N("UICorner",{CornerRadius=UDim.new(1,0),Parent=bg})
	local fl=N("Frame",{Size=UDim2.new((d-mn)/(mx-mn),0,1,0),BackgroundColor3=Color3.fromRGB(0,200,255),Parent=bg})N("UICorner",{CornerRadius=UDim.new(1,0),Parent=fl})
	local kn=N("Frame",{Size=UDim2.new(0,14,0,14),Position=UDim2.new((d-mn)/(mx-mn),-7,0.5,-7),BackgroundColor3=Color3.new(1,1,1),Parent=bg})N("UICorner",{CornerRadius=UDim.new(1,0),Parent=kn})
	local dr=false
	local function upd(inp)local ps=math.clamp((inp.Position.X-bg.AbsolutePosition.X)/bg.AbsoluteSize.X,0,1)local v=math.floor(mn+(mx-mn)*ps)fl.Size=UDim2.new(ps,0,1,0)kn.Position=UDim2.new(ps,-7,0.5,-7)lb.Text=i.." "..n..": "..v cb(v)return v end
	kn.InputBegan:Connect(function(i)if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then dr=true end end)
	bg.InputBegan:Connect(function(i)if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then upd(i)dr=true end end)
	U.InputChanged:Connect(function(i)if dr and(i.UserInputType==Enum.UserInputType.MouseMovement or i.UserInputType==Enum.UserInputType.Touch)then upd(i)end end)
	U.InputEnded:Connect(function()dr=false end)
end

local function CBtn(p,n,i,cb)
	local f=N("Frame",{Size=UDim2.new(1,0,0,38),BackgroundColor3=Color3.fromRGB(28,38,58),Parent=p})N("UICorner",{CornerRadius=UDim.new(0,10),Parent=f})
	N("TextLabel",{Size=UDim2.new(0,24,0,24),Position=UDim2.new(0,10,0.5,-12),BackgroundTransparency=1,Text=i,TextSize=15,Parent=f})
	N("TextLabel",{Size=UDim2.new(0.55,0,1,0),Position=UDim2.new(0,38,0,0),BackgroundTransparency=1,Text=n,TextColor3=Color3.new(1,1,1),TextSize=11,Font=Enum.Font.Gotham,TextXAlignment=Enum.TextXAlignment.Left,Parent=f})
	local b=N("TextButton",{Size=UDim2.new(0,56,0,24),Position=UDim2.new(1,-68,0.5,-12),BackgroundColor3=Color3.fromRGB(0,180,255),Text="CLICK",TextColor3=Color3.new(1,1,1),TextSize=10,Font=Enum.Font.GothamBold,AutoButtonColor=false,Parent=f})N("UICorner",{CornerRadius=UDim.new(0,6),Parent=b})
	b.MouseButton1Click:Connect(function()Tw(b,{BackgroundColor3=Color3.fromRGB(0,255,150)},0.1)task.delay(0.15,function()Tw(b,{BackgroundColor3=Color3.fromRGB(0,180,255)},0.2)end)cb()end)
end

-- Server functions
local function SHop()local s,e=pcall(function()return H:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/"..game.PlaceId.."/servers/Public?sortOrder=Asc&limit=100"))end)if s and e and e.data then for _,v in pairs(e.data)do if v.playing<v.maxPlayers and v.id~=game.JobId then TP:TeleportToPlaceInstance(game.PlaceId,v.id)return end end end end

-- WORKING OUTLINE ESP
local function ClearESP()
	for _,pl in pairs(P:GetPlayers())do
		if pl~=LP and pl.Character then
			local hl=pl.Character:FindFirstChild("Totally NOT Esp")
			local ic=pl.Character:FindFirstChild("Icon")
			if hl then hl:Destroy()end
			if ic then ic:Destroy()end
		end
	end
end

local function RunESP()
	spawn(function()
		while En.ESP do
			for _,v in pairs(P:GetPlayers())do
				if v~=LP and v.Character and v.Character:FindFirstChild("HumanoidRootPart")then
					local pos=0
					if LP.Character and LP.Character:FindFirstChild("HumanoidRootPart")then
						pos=math.floor((LP.Character.HumanoidRootPart.Position-v.Character.HumanoidRootPart.Position).magnitude)
					end
					local shouldShow=not En.TC or (En.TC and v.TeamColor~=LP.TeamColor)
					if shouldShow then
						if not v.Character:FindFirstChild("Totally NOT Esp")then
							local ESP=N("Highlight",{Name="Totally NOT Esp",Adornee=v.Character,Enabled=true,DepthMode=Enum.HighlightDepthMode.AlwaysOnTop,FillColor=v.TeamColor.Color,FillTransparency=0.5,OutlineColor=Color3.fromRGB(255,255,255),OutlineTransparency=0,Parent=v.Character})
							local Icon=N("BillboardGui",{Name="Icon",Active=true,AlwaysOnTop=true,ExtentsOffset=Vector3.new(0,1,0),LightInfluence=1,Size=UDim2.new(0,800,0,50),Parent=v.Character})
							N("TextLabel",{Name="ESP Text",BackgroundTransparency=1,Size=UDim2.new(0,800,0,50),Font=Enum.Font.SciFi,Text=v.Name.." | Distance: "..pos,TextColor3=v.TeamColor.Color,TextSize=18,TextWrapped=true,Parent=Icon})
						else
							local ic=v.Character:FindFirstChild("Icon")
							if ic and ic:FindFirstChild("ESP Text")then ic["ESP Text"].Text=v.Name.." | Distance: "..pos end
						end
					else
						local hl=v.Character:FindFirstChild("Totally NOT Esp")
						local ic=v.Character:FindFirstChild("Icon")
						if hl then hl.Enabled=false end
						if ic then ic.Enabled=false end
					end
				end
			end
			wait(0.1)
		end
		ClearESP()
	end)
end

-- WORKING FLY V3
local function StartFlyV3()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/XNEOFF/FlyGuiV3/main/FlyGuiV3.txt"))()
end

-- WORKING FAKE LAG (stutter movement)
local FakeLagConn=nil
local function eFL()
	if FakeLagConn then FakeLagConn:Disconnect()end
	FakeLagConn=R.RenderStepped:Connect(function()
		if not En.L then return end
		if math.random(1,15)==1 then
			if HRP then
				local lastC=HRP.CFrame
				HRP.Anchored=true
				wait(math.random(50,200)/1000)
				if HRP then
					HRP.Anchored=false
					HRP.CFrame=lastC
				end
			end
		end
	end)
end
local function dFL()if FakeLagConn then FakeLagConn:Disconnect()FakeLagConn=nil end if HRP then HRP.Anchored=false end end

-- WORKING NO PING (reduce network requests)
local NoPingConn=nil
local function eNP()
	if NoPingConn then NoPingConn:Disconnect()end
	NoPingConn=R.RenderStepped:Connect(function()
		if En.NP then
			settings().Network.IncomingReplicationLag=0.08
		else
			settings().Network.IncomingReplicationLag=0
		end
	end)
end
local function dNP()if NoPingConn then NoPingConn:Disconnect()NoPingConn=nil end settings().Network.IncomingReplicationLag=0 end

-- Noclip
local function eN()if NC.c then NC.c:Disconnect()end NC.c=R.Stepped:Connect(function()if En.N and Cht then for _,p in pairs(Cht:GetDescendants())do if p:IsA("BasePart")then p.CanCollide=false end end end end)end
local function dN()if NC.c then NC.c:Disconnect()NC.c=nil end if Cht then for _,p in pairs(Cht:GetDescendants())do if p:IsA("BasePart")then p.CanCollide=true end end end end

-- Fullbright
local OB,OG,OO,OT=L.Brightness,L.GlobalShadows,L.OutdoorAmbient,L.Ambient
local function eB()if FBConn then FBConn:Disconnect()end FBConn=R.RenderStepped:Connect(function()if En.B then L.Brightness=3 L.GlobalShadows=false L.OutdoorAmbient=Color3.new(1,1,1)L.Ambient=Color3.new(1,1,1)for _,o in pairs(L:GetDescendants())do if o:IsA("Atmosphere")then o.Density=0 end end end end)end
local function dB()if FBConn then FBConn:Disconnect()FBConn=nil end L.Brightness=OB L.GlobalShadows=OG L.OutdoorAmbient=OO L.Ambient=OT end

-- No Fog
local function eNF()if NFConn then NFConn:Disconnect()end NFConn=R.RenderStepped:Connect(function()if En.NF then L.FogEnd=100000 L.FogStart=0 for _,o in pairs(L:GetDescendants())do if o:IsA("Atmosphere")then o.Haze=0 o.Glare=0 o.FogColor=Color3.new(1,1,1)o.FogStart=0 o.FogEnd=100000 end if o:IsA("FogEffect")then o.Enabled=false end if o:IsA("BlurEffect")then o.Enabled=false end end end end)end
local function dNF()if NFConn then NFConn:Disconnect()NFConn=nil end L.FogEnd=100000 end

-- X-Ray
local function eX()if XRConn then XRConn:Disconnect()end for _,p in pairs(W:GetDescendants())do if p:IsA("BasePart")and not p:IsDescendantOf(Cht)and p.Transparency<1 and p.Name~="HumanoidRootPart"then XP[p]={t=p.Transparency,c=p.Color,m=p.Material}p.Transparency=0.6 p.Color=Color3.new(0.5,0.5,0.5)p.Material=Enum.Material.ForceField end end XRConn=R.RenderStepped:Connect(function()if En.X then for p,d in pairs(XP)do if p then p.Transparency=0.6 end end end end)end
local function dX()if XRConn then XRConn:Disconnect()XRConn=nil end for p,d in pairs(XP)do if p then p.Transparency=d.t p.Color=d.c p.Material=d.m end end XP={}end

-- Aimbot
local function gCP()local cp,sd,mp=nil,V.AF,U:GetMouseLocation()local cam=W.CurrentCamera for _,pl in pairs(P:GetPlayers())do if pl==LP or(En.T and pl.Team==LP.Team)then continue end local ch=pl.Character local hm=ch and ch:FindFirstChild("Humanoid")local hd=ch and ch:FindFirstChild("Head")if not hm or hm.Health<=0 or not hd then continue end local sp,os=cam:WorldToViewportPoint(hd.Position)if not os then continue end local d=(Vector2.new(sp.X,sp.Y)-mp).Magnitude if d<sd then sd,cp=d,pl end end return cp end
local ABC
local function eA()if ABC then ABC:Disconnect()end ABC=R.RenderStepped:Connect(function()if not En.A then return end if U:IsMouseButtonPressed(Enum.UserInputType.MouseButton2)or U:IsGamepadButtonDown(Enum.UserInputType.Gamepad1,Enum.KeyCode.ButtonL2)then local t=gCP()if t and t.Character and t.Character:FindFirstChild("Head")then local cam=W.CurrentCamera cam.CFrame=cam.CFrame:Lerp(CFrame.new(cam.CFrame.Position,t.Character.Head.Position),V.AS)end end end)end
local function dA()if ABC then ABC:Disconnect()ABC=nil end end

-- Anti-AFK
local function eK()if AFK.c then AFK.c:Disconnect()end local lt=tick()AFK.c=R.RenderStepped:Connect(function()if not En.K then return end if tick()-lt>300 then lt=tick()if Hum then Hum:ChangeState(Enum.HumanoidStateType.Jumping)end end end)end
local function dK()if AFK.c then AFK.c:Disconnect()AFK.c=nil end end

-- FNAF ESP Functions
local function ClearFNAFESP()
	for _,hl in pairs(FNAFESPModels)do
		if hl and hl.Parent then
			hl:Destroy()
		end
	end
	FNAFESPModels={}
end

local function createFNAFESP(model)
	if model:IsA("Model") then
		if not model.PrimaryPart then
			for _, child in ipairs(model:GetDescendants()) do
				if child:IsA("BasePart") then
					model.PrimaryPart = child
					break
				end
			end
			if not model.PrimaryPart then
				return
			end
		end
		
		local highlight = Instance.new("Highlight")
		highlight.Adornee = model
		highlight.Parent = C
		highlight.FillColor = Color3.fromRGB(255, 0, 0)
		highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
		highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
		table.insert(FNAFESPModels, highlight)
		
		local billboard = Instance.new("BillboardGui")
		billboard.Name = "FNAFESPName"
		billboard.Adornee = model.PrimaryPart
		billboard.Size = UDim2.new(0, 200, 0, 50)
		billboard.StudsOffset = Vector3.new(0, 3, 0)
		billboard.AlwaysOnTop = true
		billboard.Parent = C
		
		local nameLabel = Instance.new("TextLabel")
		nameLabel.Size = UDim2.new(1, 0, 1, 0)
		nameLabel.BackgroundTransparency = 1
		nameLabel.Text = model.Name
		nameLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
		nameLabel.TextSize = 14
		nameLabel.Font = Enum.Font.GothamBold
		nameLabel.Parent = billboard
		
		table.insert(FNAFESPModels, billboard)
	end
end

local function findAndApplyFNAFESP(folder)
	if folder:IsA("Folder") then
		for _, child in pairs(folder:GetChildren()) do
			if child:IsA("Model") then
				createFNAFESP(child)
			end
		end
	end
end

local function RunFNAFESP()
	spawn(function()
		while En.FNAFESP do
			local animatronicsFolder = W:FindFirstChild("Animatronics")
			if animatronicsFolder and animatronicsFolder:IsA("Folder") then
				for _, animatronicFolder in pairs(animatronicsFolder:GetChildren()) do
					findAndApplyFNAFESP(animatronicFolder)
				end
			end
			wait(1)
		end
